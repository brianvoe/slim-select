{
  "name": "slim-select",
  "description": "Slim advanced select dropdown",
  "version": "2.13.1",
  "type": "module",
  "author": "Brian Voelker <brian@webiswhatido.com> (http://webiswhatido.com)",
  "homepage": "https://slimselectjs.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brianvoe/slim-select.git"
  },
  "bugs": {
    "url": "https://github.com/brianvoe/slim-select/issues"
  },
  "keywords": [
    "select",
    "vanilla",
    "dropdown",
    "search",
    "multiselect"
  ],
  "main": "dist/slimselect.umd.js",
  "browser": "dist/slimselect.umd.js",
  "module": "dist/slimselect.es.js",
  "unpkg": "dist/slimselect.umd.min.js",
  "types": "dist/index.d.ts",
  "style": "dist/slimselect.css",
  "sass": "src/slim-select/slimselect.scss",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/slimselect.es.js",
      "require": "./dist/slimselect.umd.js"
    },
    "./vue": {
      "types": "./dist/vue/index.d.ts",
      "import": "./dist/vue/index.js",
      "require": "./dist/vue/index.umd.js"
    },
    "./styles": "./dist/slimselect.css",
    "./scss": "./src/slim-select/slimselect.scss"
  },
  "scripts": {
    "dev": "vite --config vite.config.docs.mts",
    "format": "prettier --write --cache --parser typescript \"src/**/*.ts\"",
    "build": "npm run build:clean && npm run build:docs && npm run build:library",
    "build:clean": "rimraf ./dist/*",
    "build:docs": "vite build --config vite.config.docs.mts",
    "build:library": "npm run build:library:js && npm run build:library:vue && npm run build:library:css",
    "build:library:js": "vite build --config vite.config.lib.mts",
    "build:library:vue": "vite build --config vite.config.vue.mts",
    "build:library:css": "sass src/slim-select/slimselect.scss dist/slimselect.css --style=compressed",
    "lint": "prettier --write ./src/**/*",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "release": "node release.js"
  },
  "devDependencies": {
    "@testing-library/user-event": "^14.6.1",
    "@types/downloadjs": "^1.4.6",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "axe-core": "^4.11.0",
    "clipboard": "^2.0.11",
    "downloadjs": "^1.4.7",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^27.0.1",
    "prettier": "^3.6.2",
    "prismjs": "^1.30.0",
    "rimraf": "^6.0.1",
    "sass": "^1.93.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.1.10",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3",
    "vue-tsc": "^3.1.1",
    "vuex": "^4.0.2"
  }
}
